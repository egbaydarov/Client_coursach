                 Title="Feed"
                 BackgroundColor="White"
                 NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            


                <ListView x:Name="FeedView" Grid.Row="0"  HasUnevenRows="true" CachingStrategy="RecycleElement"  ItemTapped="OnItemTapped" >
